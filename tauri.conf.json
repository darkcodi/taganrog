{
  "$schema": "https://schema.tauri.app/config/2.0.0-rc",
  "productName": "Taganrog",
  "version": "0.2.0",
  "identifier": "com.taganrog.dev",
  "build": {
    "beforeBuildCommand": "",
    "beforeDevCommand": "",
    "frontendDist": "templates"
  },
  "app": {
    "withGlobalTauri": true,
    "security": {
      "csp": null,
      "assetProtocol": {
        "scope": ["$APPDATA/**", "$RESOURCE/../**", "**"],
        "enable": true
      }
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
